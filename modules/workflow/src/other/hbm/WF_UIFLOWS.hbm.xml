<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.shaolin.bmdp.workflow.be.UIFlowsImpl" table="WF_UIFLOWS">
    <id name="id" column="ID">
      <generator class="native" />
    </id>
    <property name="name" column="NAME"/>
    <property name="flow" column="FLOW"/>
    <property name="moduleItemId" column="MODULEITEMID"/>
    <property name="moduleTypeInt" type="integer" column="MODULETYPE"/>
    <property name="_enable" type="boolean" column="_enable"/>
  </class>
</hibernate-mapping>
